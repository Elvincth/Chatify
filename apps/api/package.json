{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "cross-env NODE_ENV=development nodemon --exec \"node -r esbuild-register ./src/server.ts\" -e .ts",
    "lint": "tsc --noEmit && TIMING=1 eslint src/**/*.ts* --fix",
    "start": "cross-env NODE_ENV=production node -r esbuild-register ./src/server.ts"
  },
  "main": "src/server.ts",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.7.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.5.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^15.12.2",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-local": "^1.0.34",
    "esbuild": "^0.15.14",
    "esbuild-register": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-custom-server": "*",
    "interfaces": "*",
    "nodemon": "^2.0.20",
    "tsconfig": "*",
    "typescript": "^4.8.4"
  }
}